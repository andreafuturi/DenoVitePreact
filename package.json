{
  "name": "denovitepreact",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "npm run client & npm run server",
    "server": "deno run --unstable --allow-env --watch --no-check --allow-read --allow-ffi --allow-run --allow-write --allow-net --allow-sys=networkInterfaces server/index.jsx",
    "client": "vite client --host",
    "preview": "should automatically turn windows.prod to true and run the server only",
    "build": "vite build client && npm run move-js-file",
    "final": "deno server pack",
    "move-js-file": "mv ./client/dist/assets/index.*.js ./client/dist/assets/index.js && mv ./client/dist/assets/index.js ./client"
  },
  "dependencies": {
    "preact": "^10.8.1",
    "preact-render-to-string": "^5.2.0"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.2.0",
    "vite": "^2.9.2"
  }
}
